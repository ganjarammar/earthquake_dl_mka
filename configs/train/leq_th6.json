{
    "input_hdf5": "../data/archive/merge.hdf5",
    "input_csv": "../data/archive/merge_filtered.csv",
    "output_name": "../outputs/leq_4.4",
    "model_class": "leq",
    "input_dimension": [6000, 3],
    "cnn_blocks": 4,
    "lstm_blocks": 4,
    "padding": "same",
    "activation": "relu",
    "drop_rate": 0.1,
    "use_prelu": false,
    "shuffle": true,
    "label_type": "triangle",
    "normalization_mode": "std",
    "augmentation": false,
    "loss_weights": [0.05, 0.25, 0.70],
    "loss_types": ["binary_crossentropy", "binary_crossentropy", "binary_crossentropy"],
    "train_valid_test_split": [0.8, 0.1, 0.1],
    "mode": "generator",
    "batch_size": 32,
    "epochs": 10,
    "monitor": "val_loss",
    "patience": 12,
    "gpuid": 1
}
